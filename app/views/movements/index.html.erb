<p id="notice"><%= notice %></p>

<h1>Movements</h1>

<table>
  <thead>
    <tr>
      <th>Datetime</th>
      <th>Id item</th>
      <th>Id status</th>
      <th>Id sector</th>
      <th>Id floor</th>
      <th>Item price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movements.each do |movement| %>
      <tr>
        <td><%= movement.datetime %></td>
        <td><%= movement.id_item %></td>
        <td><%= movement.id_status %></td>
        <td><%= movement.id_sector %></td>
        <td><%= movement.id_floor %></td>
        <td><%= movement.item_price %></td>
        <td><%= link_to 'Show', movement %></td>
        <td><%= link_to 'Edit', edit_movement_path(movement) %></td>
        <td><%= link_to 'Destroy', movement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movement', new_movement_path %>
