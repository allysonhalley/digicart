<p id="notice"><%= notice %></p>

<h1>Avaible's Status</h1>

<div class="col-xs-12">
  <div class="box">
    <!-- /.box-header -->
    <div class="box-body table-responsive no-padding">
      <table class="table table-hover">
        <thead>
        <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Describe</th>
        </tr>
        </thead>
        <tbody>
        <% @statuses.each do |status| %>
            <tr>
              <td><%= status.id %></td>
              <td><%= status.name %></td>
              <td><%= status.describe %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>